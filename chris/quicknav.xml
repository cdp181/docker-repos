<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
<Category>Tools:</Category>
  <Name>quicknav</Name>
  <Overview>
    Create page with links to your devices and applications with status.
  </Overview>
  <Description>
    Create a page with links and status[br][br]
    [b]/data[/b] : sqlite database location[br]
    [b]/appdata[/b] : location of Sonarr and Radaar sqlite DBS[br]
  </Description>
  <Registry>https://hub.docker.com/r/cdp181/quicknav/</Registry>
  <GitHub>https://github.com/cdp181/quicknav</GitHub>
  <Repository>cdp181/quicknav</Repository>
  <Support></Support>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>21337</HostPort>
        <ContainerPort>21337</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/quicknav</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata</HostDir>
      <ContainerDir>/appdata</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:21337]/</WebUI>
  <Banner>http://i.imgur.com/4u0QLqM.png</Banner>
  <Icon>http://i.imgur.com/4u0QLqM.png</Icon>
</Container>
